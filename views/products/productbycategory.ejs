<img class="responsive-img" src="/images/cabecera-mano2.jpg" width="100%">

<div class="container">
<h2 class="center-align dark-grey-text"> <%= category %> </h2>
<div class="row">
  <% product.forEach((product) => { %>
  <div class="col s12 m4">
    <div class="card">
      <div class="card-image" >
        <img src="<%= product.photo %>">
        <a href="/message/<%= product.creator._id %>/new" = class="btn-floating halfway-fab waves-effect light-green"><i class="material-icons">message</i></a>
      </div>
      <div class="card-content">
        <h4 class="card-title"> <%= product.name %> </h4>
        <p><%= product.description %> </p>
        <a class="blue-lighten-1-text" href="/user/profile/<%= product.creator._id%>"><b>See <%= product.creator.username %>Â´s profile</b></a>
      </div>
    </div>
  </div>

  <% }) %>
  </div>

  </div>
