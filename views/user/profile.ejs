<!-- FALTA LOCALIZACION Y PRODUCTOS -->

<div class="container">
<h3 class=" fa fa-user center-align teal-text"> My profile</h3><br>
<ul class="collection">
  <li class="collection-item avatar">
    <img src="<%= user.image%>" width="50px" height="50px">
    </li>
  <li class="collection-item avatar">
    <p>Email: <br>
       <%= user.email %>
    </p>
  </li>
  <li class="collection-item avatar">
    <p>Username:<br>
       <%= user.username %>
    </p>
  </li>
  <li class="collection-item avatar">
    <p>Name:<br>
       <%= user.name %>
    </p>
  </li>
  <li class="collection-item avatar">
    <p>Last Name:<br>
       <%= user.lastname %>
    </p>
  </li>
  <li class="collection-item avatar">
    <p>Trust level:<br>
       <%= user.trustLevel %>
    </p>
  </li>
</ul>
 </div>
 <div class="container">
   <p>Location:</p>
   <div id="map"></div>
   <input id="pac-input" class="controls" type="text" placeholder="Enter a location">
   <input id="latitude" name="latitude" type="text">
   <input id="longitude" name="longitude" type="text">
 </div>
 <br>
 <h5 class="teal-text center-align">Reviews:</h5>
 <div class="container">
   <div class="center-btn">
     <a href="../../user/<%= user._id %>/editUser" class="btn btn-success"><span class="fa fa-sign-in"></span>EDITAR MI USUARIO</a><br></br>
</div>
</div>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyApXzFjm59CO5m3ox-cr3w3AiiVnsBHzfQ&libraries=places"></script>
<script type="text/javascript" src="/javascripts/mapDrawAll.js"></script>
